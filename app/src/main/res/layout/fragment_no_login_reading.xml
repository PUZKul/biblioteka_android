<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#74C177"
    tools:context=".ui.fragments.NoLoginReadingAndHistory.NoLoginReadingFragment">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="213dp"
        android:layout_height="182dp"
        android:clickable="true"
        android:contentDescription="@string/clicktologinin"
        android:focusable="true"
        android:src="@drawable/not_login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.307" />

    <TextView
        android:id="@+id/fragmentNoLoginReading_text_LoginIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="@string/login_in"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/FragmentNoLogin_text_historyInfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/FragmentNoLogin_text_historyInfo"
        android:layout_width="385dp"
        android:layout_height="60dp"
        android:layout_marginBottom="184dp"
        android:background="@drawable/current_page"
        android:fontFamily="sans-serif-light"
        android:text="for creating your own \n reading list"
        android:textAlignment="center"
        android:textColor="#2B2B2B"
        android:textSize="24sp"
        android:textStyle="bold"
        android:typeface="sans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="16dp" />
</androidx.constraintlayout.widget.ConstraintLayout>