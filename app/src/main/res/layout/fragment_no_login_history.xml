<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.fragments.NoLoginReadingAndHistory.NoLoginHistoryFragment">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="187dp"
        android:layout_height="168dp"
        android:clickable="true"
        android:contentDescription="@string/clicktologinin"
        android:focusable="true"
        android:src="@drawable/login"
        app:layout_constraintBottom_toTopOf="@+id/fragmentNoLoginHistory_text_LoginIn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.896" />

    <TextView
        android:id="@+id/fragmentNoLoginHistory_text_LoginIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/login_in"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="329dp" />

    <TextView
        android:id="@+id/FragmentNoLogin_text_historyInfo"
        android:layout_width="367dp"
        android:layout_height="59dp"
        android:layout_marginBottom="184dp"
        android:background="@drawable/btn_primary"
        android:fontFamily="sans-serif-light"
        android:text="for creating your own \n history list"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryBackground"
        android:textSize="24sp"
        android:textStyle="bold"
        android:typeface="sans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragmentNoLoginHistory_text_LoginIn"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="17dp" />
</androidx.constraintlayout.widget.ConstraintLayout>

